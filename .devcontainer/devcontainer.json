{
  "name": "Prism",
  "image": "oven/bun:latest",
  "updateRemoteUserUID": true,
  "features": {
      "ghcr.io/devcontainers/features/git:1": {}
  },
  "customizations": {
      "vscode": {
          "extensions": [
              "oven.bun-vscode",
              "esbenp.prettier-vscode",
              "dbaeumer.vscode-eslint"
          ],
          "settings": {
            "editor.defaultFormatter": "esbenp.prettier-vscode"
          }
      }
  }
}